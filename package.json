{
  "name": "mh-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --debug --watch",
    "dev:hot": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "build": "nest build",
    "lint": "eslint \"apps/**/*.ts\" \"libs/**/*.ts\" --fix",
    "test": "jest",
    "test:command": "nest build command-line & node ./dist/apps/command-line/main.js seed:data",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "migration:generate": "npm config set name change_db && typeorm-ts-node-commonjs migration:generate -p -d ./libs/database/src/common/data-source.ts ./libs/database/src/migrations/%npm_config_name%",
    "migration-ubuntu:generate": "typeorm-ts-node-commonjs migration:generate -p -d ./libs/database/src/common/data-source.ts ./libs/database/src/migrations/db-change",
    "migration:create": "npm config set name change_db && typeorm-ts-node-commonjs migration:create ./libs/database/src/migrations/%npm_config_name%",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./libs/database/src/common/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./libs/database/src/common/data-source.ts",
    "seed:data": "npm run build seed-data && node dist/apps/seed-data/main.js start:seed",
    "stop:local": "taskkill /f /im node.exe"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "kafkajs": "^2.2.3",
    "mysql2": "^2.3.3",
    "nest-commander": "^3.6.1",
    "nodemailer": "^6.9.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "28.1.3",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^_libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^_libs/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^_libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^_libs/transporter(|/.*)$": "<rootDir>/libs/transporter/src/$1"
    }
  }
}
