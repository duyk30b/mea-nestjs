{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-public/src",
  "monorepo": true,
  "root": "apps/api-public",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api-public/tsconfig.app.json"
  },
  "projects": {
    "api-public": {
      "type": "application",
      "root": "apps/api-public",
      "entryFile": "main",
      "sourceRoot": "apps/api-public/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-public/tsconfig.app.json"
      }
    },
    "event-dispatcher": {
      "type": "application",
      "root": "apps/event-dispatcher",
      "entryFile": "main",
      "sourceRoot": "apps/event-dispatcher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-dispatcher/tsconfig.app.json"
      }
    },
    "worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    },
    "seed-data": {
      "type": "application",
      "root": "apps/seed-data",
      "entryFile": "main",
      "sourceRoot": "apps/seed-data/src",
      "compilerOptions": {
        "tsConfigPath": "apps/seed-data/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "transporter": {
      "type": "library",
      "root": "libs/transporter",
      "entryFile": "index",
      "sourceRoot": "libs/transporter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/transporter/tsconfig.lib.json"
      }
    }
  }
}
